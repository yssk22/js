{
  "parser": "babel-eslint",
  "extends": "google",
  "plugins": ["flowtype", "react"],
  "rules": {
    "no-invalid-this": "off",
    "arrow-parens": "off",
    "require-jsdoc": "off",
    "comma-dangle": "off",
    "max-len": ["error", { "code": 120 }],
    "object-curly-spacing": "off",
    "no-unused-vars": "error",
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1
  }
}
